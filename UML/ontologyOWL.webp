# Data Properties
#################

:timestamp a owl:DatatypeProperty ;
  rdfs:domain :Event ;
  rdfs:range xsd:dateTime .

:metricTimestamp a owl:DatatypeProperty ;
  rdfs:domain :Metric ;
  rdfs:range xsd:dateTime .

:conditionTimestamp a owl:DatatypeProperty ;
  rdfs:domain :Condition ;
  rdfs:range xsd:dateTime .

:userStateTimestamp a owl:DatatypeProperty ;
  rdfs:domain :UserState ;
  rdfs:range xsd:dateTime .

:value a owl:DatatypeProperty ;
  rdfs:domain :Metric ;
  rdfs:range xsd:float .

:conditionValue a owl:DatatypeProperty ;
  rdfs:domain :Condition ;
  rdfs:range xsd:float .

:weight a owl:DatatypeProperty ;
  rdfs:domain :UserTraitWeight ;
  rdfs:range xsd:float .

:MechanicUserTraitMappingValue a owl:DatatypeProperty ;
  rdfs:domain :MechanicUserTraitMapping ;
  rdfs:range xsd:float .

:toggle a owl:DatatypeProperty ;
  rdfs:domain :AssistantAction ;
  rdfs:range xsd:boolean .

:stateChange a owl:DatatypeProperty ;
  rdfs:domain :AssistantAction ;
  rdfs:range xsd:boolean .

#############################
# Minimal OWL Restrictions
#############################

:MotivationalMechanic
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :hasMechanicState ;
    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass :MechanicState
  ] .

:EventCollection
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :storesEvent ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass :Event
  ] .

:User
  rdfs:subClassOf
    [ a owl:Restriction ;
      owl:onProperty :hasUserState ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :UserState
    ] ,
    [ a owl:Restriction ;
      owl:onProperty :hasUserTraitWeight ;
      owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :UserTraitWeight
    ] ,
    [ a owl:Restriction ;
      owl:onProperty :hasPlan ;
      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onClass :Plan
    ] .

:UserTraitWeight
  rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty :referencesTrait ;
    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
    owl:onClass :UserTrait
  ] .

